    "   \"source\": [\n",
    "    \"class DayFourPartOne(override val testResult: String = \\\"13\\\") : Problem(\\\"day4.txt\\\") {\\n\",\n",
    "    \"    private val d = listOf(\\n\",\n",
    "    \"        -1 to -1,\\n\",\n",
    "    \"        -1 to 0,\\n\",\n",
    "    \"        -1 to 1,\\n\",\n",
    "    \"        0 to -1,\\n\",\n",
    "    \"        0 to 1,\\n\",\n",
    "    \"        1 to -1,\\n\",\n",
    "    \"        1 to 0,\\n\",\n",
    "    \"        1 to 1,\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"\\n\",\n",
    "    \"    override fun solve(path: String): String {\\n\",\n",
    "    \"        val matrix = File(path).useLines { lines ->\\n\",\n",
    "    \"            val lineList = lines.toList()\\n\",\n",
    "    \"            val height = lineList.size + 2\\n\",\n",
    "    \"            val width = lineList[0].length + 2\\n\",\n",
    "    \"            Array(height) { row ->\\n\",\n",
    "    \"                Array(width) { column ->\\n\",\n",
    "    \"                    if (row == 0 || row == height - 1 || column == 0 || column == width - 1) {\\n\",\n",
    "    \"                        EMPTY\\n\",\n",
    "    \"                    } else {\\n\",\n",
    "    \"                        lineList[row - 1][column - 1]\\n\",\n",
    "    \"                    }\\n\",\n",
    "    \"                }\\n\",\n",
    "    \"            }\\n\",\n",
    "    \"        }\\n\",\n",
    "    \"\\n\",\n",
    "    \"        var acc = 0\\n\",\n",
    "    \"        for (row in 1..(matrix.size - 2)) {\\n\",\n",
    "    \"            for (col in 1..(matrix[0].size - 2)) {\\n\",\n",
    "    \"                if (matrix[row][col] == ROLL && d.sumOf { (i, j) -> if (matrix[row + i][col + j] == ROLL) 1 else 0 } < 4) {\\n\",\n",
    "    \"                    acc++\\n\",\n",
    "    \"                } else {\\n\",\n",
    "    \"                }\\n\",\n",
    "    \"            }\\n\",\n",
    "    \"        }\\n\",\n",
    "    \"        return acc.toString()\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"\\n\",\n",
    "    \"    companion object {\\n\",\n",
    "    \"        private const val ROLL = '@'\\n\",\n",
    "    \"        private const val EMPTY = '.'\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"}\\n\",\n",
    "    \"\\n\",\n",
    "    \"DayFourPartOne().run()\"\n",
