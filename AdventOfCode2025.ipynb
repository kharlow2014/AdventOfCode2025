    "    }\n",
    "   },\n",
    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class DayTwoPartTwo(override val testResult: String = \\\"4174379265\\\") : Problem(\\\"day2.txt\\\") {\\n\",\n",
    "    \"    override fun solve(path: String): String {\\n\",\n",
    "    \"        var acc = 0L\\n\",\n",
    "    \"        File(path).useLines { lines ->\\n\",\n",
    "    \"            lines.first().split(\\\",\\\").map { it.split(\\\"-\\\") }.forEach { split ->\\n\",\n",
    "    \"                for (i in split[0].toLong()..split[1].toLong()) {\\n\",\n",
    "    \"                    if (i < 10) continue\\n\",\n",
    "    \"                    val str = i.toString()\\n\",\n",
    "    \"                    if (str.all { it == str.first() }) {\\n\",\n",
    "    \"                        acc += i\\n\",\n",
    "    \"                        continue\\n\",\n",
    "    \"                    }\\n\",\n",
    "    \"\\n\",\n",
    "    \"                    for (j in (str.length / 2) downTo 2) {\\n\",\n",
    "    \"                        val chunked = str.chunked(j)\\n\",\n",
    "    \"                        if (chunked.size > 1 && chunked.all { it == chunked.first() }) {\\n\",\n",
    "    \"                            acc += i\\n\",\n",
    "    \"                            break\\n\",\n",
    "    \"                        }\\n\",\n",
    "    \"                    }\\n\",\n",
    "    \"                }\\n\",\n",
    "    \"            }\\n\",\n",
    "    \"        }\\n\",\n",
    "    \"        return acc.toString()\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"}\\n\",\n",
    "    \"\\n\",\n",
    "    \"DayTwoPartTwo().run()\"\n",
    "   ],\n",
    "   \"outputs\": [\n",
    "    {\n",
    "     \"name\": \"stdout\",\n",
