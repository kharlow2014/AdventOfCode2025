    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"import java.io.File\\n\",\n",
    "    \"abstract class Problem(private val filename: String) {\\n\",\n",
    "    \"    abstract val testResult: String\\n\",\n",
    "    \"    fun run() {\\n\",\n",
    "    \"        val testOutput = solve(\\\"testData/$filename\\\")\\n\",\n",
    "    \"        val actualOutput = solve(\\\"data/$filename\\\")\\n\",\n",
    "    \"        println(\\\"Test results: Expected=$testResult Actual=$testOutput\\\")\\n\",\n",
    "    \"        println(\\\"Solution: $actualOutput\\\")\\n\",\n",
    "    \"    }\\n\",\n",
    "    \"    abstract fun solve(path: String): String\\n\",\n",
    "    \"}\"\n",
    "   ],\n",
    "   \"outputs\": [],\n",
